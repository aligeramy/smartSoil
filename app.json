{
  "expo": {
    "name": "SmartSoil",
    "slug": "smart-soil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "smartsoil",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#013B1A"
    },
    "newArchEnabled": true,
    "primaryColor": "#34C759",
    "backgroundColor": "#F5F5F7",
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/ios/ios-tinted.png",
      "alternateIcon": {
        "light": "./assets/images/ios/ios-light.png",
        "dark": "./assets/images/ios/ios-dark.png"
      },
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.softxinnovations.smartsoil",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#063B1D"
      },
      "edgeToEdgeEnabled": true,
      "navigationBarColor": "#063B1D",
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": true,
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "INTERNET"
      ],
      "statusBar": {
        "backgroundColor": "#063B1D",
        "barStyle": "light-content",
        "translucent": false
      },
      "package": "com.softxinnovations.smartsoil",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "smartsoil"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#013B1A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "334467c2-f1d5-4a0e-b474-061f1ab4d9c9"
      }
    }
  }
}
